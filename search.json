---
layout: none
---
[

  {% for collection in site.collections %}
  {% for colecao in collection.docs %}
    {
    "title"    : "{{ site.data[site.active_lang]['messages'][colecao.title] | escape }}",
    "category" : "{{ colecao.category }}",
    "tags"     : "{{ colecao.tags | join: ', ' }}",
    "url"      : "{{ site.baseurl }}{% if site.active_lang != site.default_lang %}/{{site.active_lang}}{% endif %}{{ colecao.url }}",
    "date"     : "{{ colecao.date }}",
    "desc"     : "{{site.data[site.active_lang]['messages'][colecao.summary]}}"
    },
  {% endfor %}
  {% endfor %}

    {
    }

  ]
